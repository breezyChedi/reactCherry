{
  "expo": {
    "name": "reactCherry",
    "slug": "reactCherry",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./icons/cherry-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./icons/cherry-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cherry.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./icons/cherry-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cherry.app"
    },
    "web": {
      "favicon": "./icons/cherry-icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-splash-screen",
      "expo-font"
    ],
    "scheme": "react-cherry",
    "experiments": {
      "typedRoutes": false,
      "tsconfigPaths": true
    },
    "newArchEnabled": true
  }
} 